    "def solution(a, b):\n",
    "    month = [31, 29, 31, 30, 31, 30,\n",
    "            31, 31, 30, 31, 30, 31]\n",
    "    day = ['FRI', 'SAT', 'SUN', 'MON', 'TUE', 'WED', 'THU']\n",
    "    \n",
    "    current = sum(month[:a-1]) + b\n",
    "    answer = day[current % 7 - 1]\n",
    "    \n",
    "    return answer"
   ]
